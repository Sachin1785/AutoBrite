{
  "manifest_version": 3,
  "name": "Log Current Tab",
  "version": "1.0",
  "description": "Logs the currently open tab in the Chrome window.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:1108/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}